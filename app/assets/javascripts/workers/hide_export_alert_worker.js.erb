/**
 * Web Worker to hide the alert with the state of an OPML export job in a separate thread.
 */

importScripts('<%= asset_path 'workers/common/do_put' %>');

// Callback for messages from the main thread
onmessage = function(e){
  // CSRF token
  var token = e.data.token;

  // Data to send in the PUT
  var data = {opml_export: {show_alert: 'false'}};

  do_put("hide_export_alert", "/api/opml_exports.json", token, data, 0);
}