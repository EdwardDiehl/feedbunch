<%
  if user.data_import.present?
    status = user.data_import.status
  else
    status = 'NONE'
  end
%>

{
  "status": "<%= status %>"
<% if status == DataImport::RUNNING %>
,
  "import": {
    "processed": <%= user.data_import.processed_feeds %>,
    "total": <%= user.data_import.total_feeds %>
  }
<% end %>
}