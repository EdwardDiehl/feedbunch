{
  "id": "<%= job_status.id %>",
  "feed": {
    <% feed = Feed.find job_status.feed_id %>
    "id": "<%= feed.id %>",
    "title": <%= feed.title.to_json.html_safe %>,
    "url": "<%= feed.url %>",
    "folder_id": "<%= feed.user_folder(user).try(:id) || 'none' %>",
    "unread_entries": <%= user.feed_unread_count feed %>
  },
  "status": "<%= job_status.status %>"
}