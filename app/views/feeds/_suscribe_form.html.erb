<div id="suscribe-feed" class="modal hide fade">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal">&times;</button>
    <h3><%= t 'feeds.index.suscribe_title' %></h3>
  </div>
  <div class="modal-body">
    <%= simple_form_for :subscription, url: "/NOTIMPLEMENTED",
                        html: {:method => :post, class: 'form-horizontal'},
                        wrapper: :prepend do |f| %>
      <%= f.input :rss, as: :rss_icon, required: false, id: 'rss-input' %>
    <% end %>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" ><%= t 'feeds.index.close' %></button>
    <button class="btn btn-primary"><%= t 'feeds.index.suscribe_submit' %></button>
  </div>
</div>