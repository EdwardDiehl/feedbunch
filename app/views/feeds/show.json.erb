{
"unread_entries": <%= user.feed_unread_count feed %>,
"entries": [
<% entries.each do |entry| %>
{
  "id": "<%= entry.id %>",
  "title": <%= entry.title.to_json.html_safe %>,
  "read": <%= entry.read_by? user %>,
  "published": "<%= l entry.published, format: :short if entry.published.present? %>",
  "url": "<%= entry.url %>",
  "guid": "<%= entry.guid %>",
  "content": <%= entry.content.try(:to_json).html_safe %>,
  "summary": <%= entry.summary.try(:to_json).html_safe %>
}
  <% if entry != entries.last %>
,
  <% end %>
<% end %>
]
}