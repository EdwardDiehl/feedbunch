<li id="folder-{{folder.id}}-all-feeds"
    ng-class="{active: current_folder.id==folder.id}"
    ng-show="show_all_subscriptions(folder)">
  <a data-sidebar-feed
     data-feed-id="all"
     ng-click="set_current_folder(folder)">
    <em><strong>
      <i class="fa fa-rss pull-left"></i>
      <span class="sidebar-feed-title"><%= t 'feeds.index.all_entries' %></span>
    </strong></em>
    <span class="badge pull-right">{{folder_unread_entries(folder) | numberBadgeFltr}}</span>
  </a>
</li>